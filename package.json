{
    "name": "nativecolorpicker",
    "displayName": "Native Color Picker",
    "description": "Open Native Color Picker to select any color",
    "version": "0.0.5",
    "publisher": "elbert",
    "engines": {
        "vscode": "^1.14.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.pickColor"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [{
            "command": "extension.pickColor",
            "title": "Pick Color from Native Picker"
        }]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "eslint": "^3.6.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}